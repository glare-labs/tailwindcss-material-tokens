{
  "displayName": "Tailwindcss Plugin Material-Tokens",
  "name": "@glare-labs/tailwindcss-material-tokens",
  "version": "0.0.5",
  "publishConfig": {
    "access": "public"
  },
  "description": "A tailwindcss plugin that imports material tokens",
  "keywords": [
    "material",
    "tokens",
    "material-design",
    "material-tokens",
    "tailwindcss",
    "plugin",
    "tailwindcss-plugin"
  ],
  "homepage": "https://github.com/glare-labs/tailwindcss-material-tokens",
  "bugs": {
    "url": "https://github.com/glare-labs/tailwindcss-material-tokens/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glare-labs/tailwindcss-material-tokens.git"
  },
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "files": [
    "build/**/*"
  ],
  "exports": {
    ".": {
      "import": "./build/index.esm.js",
      "require": "./build/index.cjs.js",
      "types": "./build/index.d.ts",
      "default": "./build/index.esm.js"
    },
    "./*": {
      "import": "./*.js",
      "require": "./*.js",
      "types": "./index.d.ts",
      "default": "./*.js"
    }
  },
  "types": "/build/index.d.ts",
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "rollup --config",
    "fix": "gts fix",
    "prepare": "npm.cmd run compile",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run lint"
  },
  "author": "bre97-web",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "20.8.2",
    "gts": "^5.2.0",
    "rollup": "^4.13.0",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.2.0"
  }
}
