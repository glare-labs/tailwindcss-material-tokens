{
  "displayName": "Tailwindcss Plugin Material-Tokens",
  "name": "@glare-labs/tailwindcss-material-tokens",
  "version": "0.2.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "A tailwindcss plugin that imports material tokens",
  "keywords": [
    "material",
    "tokens",
    "material-design",
    "material-tokens",
    "tailwindcss",
    "plugin",
    "tailwindcss-plugin"
  ],
  "homepage": "https://github.com/glare-labs/tailwindcss-material-tokens",
  "bugs": {
    "url": "https://github.com/glare-labs/tailwindcss-material-tokens/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glare-labs/tailwindcss-material-tokens.git"
  },
  "main": "build/index.js",
  "module": "build/index.js",
  "files": [
    "build/**/*"
  ],
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.js",
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    },
    "./*": {
      "import": "./*.js",
      "require": "./*.js",
      "types": "./index.d.ts",
      "default": "./*.js"
    }
  },
  "types": "build/index.d.ts",
  "typings": "build/index.d.ts",
  "workspaces": [
    "dev-app"
  ],
  "scripts": {
    "dev": "wireit",
    "compile": "wireit"
  },
  "author": "bre97-web",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "22.5.3",
    "rollup": "^4.21.2",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.5.4",
    "wireit": "^0.14.9"
  },
  "wireit": {
    "dev": {
      "command": "npm run dev -w dev-app",
      "files": [
        "src/**/*",
        "dev-app/**/*"
      ]
    },
    "compile": {
      "command": "npx rollup --config",
      "files": [
        "src/**/*"
      ],
      "output": [
        "build/**/*"
      ],
      "clean": true
    }
  }
}
