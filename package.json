{
  "displayName": "Tailwindcss Plugin Material-Tokens",
  "name": "@glare-labs/tailwindcss-material-tokens",
  "version": "0.0.4",
  "publishConfig": {
    "access": "public"
  },
  "description": "A tailwindcss plugin that imports material tokens",
  "keywords": [
    "material",
    "tokens",
    "material-design",
    "material-tokens",
    "tailwindcss",
    "plugin",
    "tailwindcss-plugin"
  ],
  "homepage": "https://github.com/glare-labs/tailwindcss-material-tokens",
  "bugs": {
    "url": "https://github.com/glare-labs/tailwindcss-material-tokens/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glare-labs/tailwindcss-material-tokens.git"
  },
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "files": [
    "build/**/*"
  ],
  "exports": {
    "import": "./build/esm/index.js",
    "require": "./build/cjs/index.js",
    "types": "./build/types/index.d.ts",
    "node": "./build/cjs/index.js"
  },
  "types": "/build/types/index.d.ts",
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "npm run c:cjs && npm run c:esm && npm run c:types",
    "c:cjs": "npx tsc --module commonjs --outdir build/cjs --declaration false",
    "c:esm": "npx tsc --module esnext  --moduleresolution bundler --outdir build/esm --declaration false",
    "c:types": "npx tsc --module esnext  --moduleresolution bundler --outdir build/types --emitdeclarationonly",
    "fix": "gts fix",
    "prepare": "npm.cmd run compile",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run lint"
  },
  "author": "bre97-web",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@types/node": "20.8.2",
    "gts": "^5.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.2.0"
  }
}
